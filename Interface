public class OOPS{
    public static void main(String args[]){
        Queen q = new Queen();
        q.moves();
        
    }
    
}
    
    interface ChessPlayer{
       void moves();
    }
    class Queen implements ChessPlayer{
        public void moves(){
            System.out.print("up,down,right ,left,diagonal in all direction");
        }
    }
    class King implements ChessPlayer{
        public void moves(){
             System.out.print("up,down,right ,left,by 1 step");
        }
    }
       
